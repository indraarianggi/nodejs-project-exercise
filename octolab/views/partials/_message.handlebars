{{#if success_msg}}
    <div class="row">
        <div class="col s12 m12">
            <div class="alert alert-danger">
                <i class="fas fa-check-circle"></i> {{success_msg}}
            </div>
        </div>
    </div>
{{/if}}

{{#if error_msg}}
    <div class="row">
        <div class="col s12 m12">
            <div class="alert alert-danger">
                <i class="fas fa-exclamation-circle"></i> {{error_msg}}
            </div>
        </div>
    </div>
{{/if}}
