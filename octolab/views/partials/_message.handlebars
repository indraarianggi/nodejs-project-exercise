{{#if success_msg}}
    <div class="row">
        <div class="col s12 m12">
            <div class="alert alert-danger">
                {{success_msg}}
            </div>
        </div>
    </div>
{{/if}}

{{#if error_msg}}
    <div class="row">
        <div class="col s12 m12">
            <div class="alert alert-danger">
                {{error_msg}}
            </div>
        </div>
    </div>
{{/if}}
