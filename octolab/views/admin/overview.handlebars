<div class="row">
    <div class="col s12 m4">
        <div class="card">
            <div class="card-image center">
                <i class="fas fa-users fa-10x"></i>
            </div>
            <div class="card-content center">
                <h5>{{numUser}} users</h5>
            </div>
        </div>
    </div>
    <div class="col s12 m4">
        <div class="card">
            <div class="card-image center">
                <i class="fas fa-clipboard-list fa-10x"></i>
            </div>
            <div class="card-content center">
                <h5>{{numIncomingOrder}} incoming orders</h5>
            </div>
        </div>
    </div>
    <div class="col s12 m4">
        <div class="card">
            <div class="card-image center">
                <i class="fas fa-clipboard-check fa-10x"></i>
            </div>
            <div class="card-content center">
                <h5>{{numFinishedOrder}} finished orders</h5>
            </div>
        </div>
    </div>
</div>
